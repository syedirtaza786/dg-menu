{
    "name": "syedirtaza786/dg-menu",
    "description": "",
    "keywords": [
        "syedirtaza786",
        "dg-menu"
    ],
    "homepage": "https://github.com/syedirtaza786/dg-menu",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Irtaza",
            "email": "syedirtaza@digitalgravity.ae",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Syedirtaza786\\DgMenu\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Syedirtaza786\\DgMenu\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Syedirtaza786\\DgMenu\\DgMenuServiceProvider"
            ],
            "aliases": {
                "DgMenu": "Syedirtaza786\\DgMenu\\DgMenuFacade"
            }
        }
    }
}
